{
  "name": "Tab Assassin (by Google)",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Achieve tabstrip zen",
  "background_page": "background.html",
  "options_page": "options.html",
  "icons": {
    "128": "images/ta-icon-128.png"
  },
  "browser_action": {
    "default_icon": "images/ta-icon.png",
    "default_title": "Tab Assassin",
    "default_popup": "TabAssassin.html"
  },
  "permissions": [
    "tabs",
    "background",
    "idle"
  ]
}
